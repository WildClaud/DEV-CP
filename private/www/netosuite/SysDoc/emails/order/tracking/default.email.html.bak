      <td valign="top">[@ship_method@]</td>
      <td colspan="2" valign="top"><!--##[%DATA id:'ship_tracking' if:'ne' value:'' %]##-->[@ship_tracking@]<!--##[%END DATA%]##-->
     	 <!--##[%DATA id:'ship_tracking' if:'eq' value:'' %]##--> <!--##[%END DATA%]##-->
        <!--##[%DATA id:'tracking_url' if:'ne' value:'' %]##--> <br /><a href="[@tracking_url@]" target="_blank">Online Tracking</a><!--##[%END DATA%]##-->
        <!--##[%DATA id:'tracking_url' if:'eq' value:'' %]##--> <!--##[%END DATA%]##--></td>
  </tr> 
    <!--##[%multi_item_pack id:'[@order_id@]' counter:'[@counter@]'%][%PARAM *header%]##-->
